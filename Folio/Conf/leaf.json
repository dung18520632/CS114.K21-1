{
	/****
	* DATASET PATHS
	****/
	"image_dataset": "Dataset",

	/****
	* FEATURE EXTRACTION
	****/
	"features_path": "output/leaf/leaf_features.hdf5",
	"percent_gt_images": 1,
	"offset": 5,
	"use_flip": true,

	/****
	* HISTOGRAM OF ORIENTED GRADIENTS DESCRIPTOR
	****/
	"orientations": 9,
	"pixels_per_cell": [4, 4],
	"cells_per_block": [2, 2],
	"normalize": true,

	/****
	* OBJECT DETECTOR
	****/
	"window_step": 4,
	"overlap_thresh": 0.3,
	"pyramid_scale": 1.5,
	"window_dim": [96, 32],
	"min_probability": 0.7,

	/****
	* LINEAR SVM
	****/
	"classifier_path": "output/leaf/model_with_hn.cpickle",
	"C": 0.01,

	/****
	* HARD NEGATIVE MINING
	****/
	"hn_num_distraction_images": 50,
	"hn_window_step": 4,
	"hn_pyramid_scale": 1.5,
	"hn_min_probability": 0.51
}